{
	"name": "teststuff",
	"version": "1.0.0",
	"description": "test thing",
  "main": "index.js",
  "nodemonConfig": { 
    "exec": "npm run dev",
    "watch": ["./**/*.js" ],
    "ignore": ["**/__tests__/**", "*.test.js", "*.spec.js" , "./dist"]
  },
	"scripts": {
		"test": "test",
    "start": "npm run prod",
    "start2": "node ./dist/index.js",
		"transpile": "babel . --out-dir ./dist --ignore ./node_modules/**/*.js ",
    "clean": "rimraf dist",
    "build": "npm-run-all clean transpile",
    "dev": "SET NODE_ENV=development & npm-run-all build start2",
    "prod": "SET NODE_ENV=production  & npm-run-all build start2",
    "watch:dev": "nodemon"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.5",
		"nodemon": "^2.0.3",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2"
	},
	"babel": {
		"presets": [ "@babel/preset-env" ]
	}
}
